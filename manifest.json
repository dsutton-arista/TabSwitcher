{
  "manifest_version": 3,
  "name": "Tab Switcher",
  "version": "1.0",
  "description": "A Chrome extension that allows users to switch between their most recently used tabs.",
    "permissions": ["tabs", "storage"],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "switch-to-previous-tab": {
      "suggested_key": {
        "mac": "Alt+S",
        "default": "Alt+S"
      },
      "description": "Switch to the previous tab"
    },
    "cycle-back-tabs": {
      "suggested_key": {
        "mac": "Alt+P",
        "default": "Alt+P"
      },
      "description": "Cycle back through recent tabs"
    },
    "cycle-forward-tabs": {
      "suggested_key": {
        "mac": "Alt+N",
        "default": "Alt+N"
      },
      "description": "Cycle forward through recent tabs"
    },
    "cycle-back-tabs-X2": {
      "suggested_key": {
        "mac": "Alt+Shift+P",
        "default": "Alt+Shift+P"
      },
      "description": "Cycle back through twice as many recent tabs"
    },
    "cycle-forward-tabs-X2": {
      "description": "Cycle forward through twice as many recent tabs"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
